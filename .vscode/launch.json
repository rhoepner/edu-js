    "version": "0.2.0",
    "configurations": [
        {
            "name": "Lauch KOA Server",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/koa_server.js",
        },
        {
            "name": "Launch Simple Server",
            "type": "node",
            "runtimeArgs": [ "-r", "esm"],
            "request": "launch",
            "program": "${workspaceFolder}/server.js",
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 9229,
        },
        {
            "name": "Print As Json",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_print_as_json.js"
        }
    ]
